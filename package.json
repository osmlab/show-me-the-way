  },
  "devDependencies": {
    "browserify": "^17.0.0",
    "eslint": "^8.18.0",
    "esmify": "^2.1.1"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "globals": {
      "L": "readonly"
    },
    "parserOptions": {
      "sourceType": "module"
